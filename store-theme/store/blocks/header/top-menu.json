{
  "vtex.menu@2.x:menu#websites": {
    "children": [
      "menu-item#shop"
    ]
  },
  "menu-item#shop": {
    "props": {
      "id": "menu-item-shop",
      "type": "custom",
      "iconId": null,
      "highlight": false,
      "itemProps": {
        "type": "internal",
        "href": "#",
        "noFollow": false,
        "tagTitle": "MINICONF",
        "text": "MINICONF"
      }
    }
  },


  "vtex.menu@2.x:menu#institutional": {
    "children": [
      "menu-item#vtex-search",
      "menu-item#vtex-location",
      "minicart.v2",
      "IsLogged#custom",
      "locale-switcher#vtex-switcher"
    ]
  },

  "minicart.v2": {
    "children": ["minicart-base-content"]
  },

  "menu-item#vtex-location": {
    "props": {
      "id": "menu-item-location-icon",
      "type": "custom",
      "iconProps": {
        "id": "hpa-location-marker",
        "iconPosition": "center",
        "size": 19,
        "viewBox": "0 0 15 20"

      },
      "itemProps": {
        "type": "internal",
        "href": "/location"
      }
    }
  },
  "menu-item#vtex-heart": {
    "props": {
      "id": "menu-item-location-icon",
      "type": "custom",
      "iconProps": {
        "id": "mpa-heart",
        "iconPosition": "center",
        "size": 22,
        "viewBox": "0 0 22 20"

      },
      "itemProps": {
        "type": "internal",
        "href": "/heart"
      }
    }
  },
  "menu-item#vtex-search": {
    "props": {
      "id": "vtex-search",
      "type": "custom",
      "iconProps": {
        "id": "hpa-search",
        "iconPosition": "center",
        "size": 19,
        "viewBox": "0 0 19 19"
      },
      "highlight": false,
      "itemProps": {
        "type": "internal",
        "href": "/tutti-i-prodotti"
      }
    }
  },
  "menu-item#vtex-cart": {
    "props": {
      "id": "vtex-cart",
      "type": "custom",
      "iconProps": {
        "id": "hpa-cart",
        "iconPosition": "center",
        "size": 22,
        "viewBox": "0 0 20 20"
      },
      "highlight": false,
      "itemProps": {
        "type": "external",
        "href": "http://vtex.com"
      }
    }
  },

  "menu-item#vtex-myIdoGrey": {
    "props": {
      "id": "menu-item-myIdo",
      "type": "custom",
      "blockClass": "ido-logo-class-desktop",
      "iconProps": {
        "id": "hpa-profile",
        "iconPosition": "center",
        "size": 20,
        "viewBox": "0 0 18 22"
      },
      "highlight": true,
      "itemProps": {
        "blockClass": "ido-logo-class",
        "type": "internal",
        "href": "#",
        "noFollow": true,
        "tagTitle": "myIdo",
        "text": "My IDO"
      }
    }
  },

  "menu-item#vtex-myIdoGreen": {
    "props": {
      "id": "menu-item-myIdo",
      "type": "custom",
      "blockClass": "ido-logo-class-desktop",
      "iconProps": {
        "id": "hpa-profile",
        "iconPosition": "center",
        "size": 20,
        "viewBox": "0 0 18 22"
      },
      "highlight": true,
      "itemProps": {
        "blockClass": "ido-logo-class",
        "type": "internal",
        "href": "#",
        "noFollow": true,
        "tagTitle": "myIdo",
        "text": "My IDO"
      }
    }
  },

  "locale-switcher#vtex-switcher": {
    "props": {
      "iconProps": {
        "id": "mpa-angle--down",
        "size": 1,
        "viewBox": "0 0 1 1",
        "activeClassName": "red",
        "mutedClassName": "c-action-primary"
      },
      "iconPosition": "left"
    }
  },


  "modal-trigger#modal-login-button": {
    "children": [
      "rich-text#login",
      "modal-layout#modal-login-content"
    ]
  },
  "IsLogged#custom": {
    "blocks": [
      "IsLogged.logged#logged-content",
      "IsLogged.not-logged#not-logged-content"
    ]
  },
  "IsLogged.logged#logged-content": {
    "children": [
      "menu-item#vtex-myIdoGrey",
      "login#custom-login"
    ]
  },
  "IsLogged.not-logged#not-logged-content": {
    "children": [
      "menu-item#vtex-myIdoGreen",
      "modal-trigger#modal-login-button"
    ]
  },
  "rich-text#login": {
    "props": {
      "text": "LOGIN MY IDO"
    }
  },

  "modal-layout#modal-login-content": {
    "children": [
      "modal-header",
      "login-content#custom-login"
    ]
  },

  "login-content#custom-login": {
    "props": {
      "isInitialScreenOptionOnly": false,
      "defaultOption": 1,
      "emailAndPasswordTitle": "Accedi a MyIDO",
      "accessCodeTitle": "MyiDO",
      "emailPlaceholder": "mariorossi@gmail.it",
      "passwordPlaceholder": "* * * *",
      "showPasswordVerificationIntoTooltip": true,
      "providerPasswordButtonLabel": "ACCEDI",
      "blockClass": "login-content-desktop"
    }
  },

  "login#custom-login": {
    "props": {
      "logInButtonBehavior": "link",
      "showIconProfile": false,
      "isInitialScreenOptionOnly": true,
      "defaultOption": 0
    }
  }
}
