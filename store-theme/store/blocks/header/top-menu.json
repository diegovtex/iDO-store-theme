{
  "vtex.menu@2.x:menu#websites": {
    "children": [
      "menu-item#shop"
    ]
  },
  "menu-item#shop": {
    "props": {
      "id": "menu-item-shop",
      "type": "custom",
      "iconId": null,
      "highlight": false,
      "itemProps": {
        "type": "internal",
        "href": "#",
        "noFollow": false,
        "tagTitle": "MINICONF",
        "text": "MINICONF"
      }
    }
  },


  "vtex.menu@2.x:menu#institutional": {
    "children": [
      "menu-item#vtex-search",
      "menu-item#vtex-location",
      "menu-item#vtex-cart",
      "menu-item#vtex-myIdo",
      "locale-switcher#custom"
    ]
  },

  "menu-item#vtex-location": {
    "props": {
      "id": "menu-item-location-icon",
