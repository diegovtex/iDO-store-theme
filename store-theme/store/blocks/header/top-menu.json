      "itemProps": {
        "type": "internal",
        "href": "/location"
      }
    }
  },
  "menu-item#vtex-heart": {
    "props": {
      "id": "menu-item-location-icon",
      "type": "custom",
      "iconProps": {
        "id": "mpa-heart",
        "iconPosition": "center",
        "size": 22,
        "viewBox": "0 0 22 20"

      },
      "itemProps": {
        "type": "internal",
        "href": "/heart"
      }
    }
  },
  "menu-item#vtex-search": {
    "props": {
      "id": "vtex-search",
      "type": "custom",
      "iconProps": {
        "id": "hpa-search",
